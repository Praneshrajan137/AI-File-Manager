{
  "name": "project-2-file-manager",
  "version": "0.1.0",
  "description": "Industrial-grade OS File Manager with Custom LLM",
  "main": "dist/main/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "webpack --config webpack.main.config.js --watch",
    "dev:renderer": "webpack serve --config webpack.renderer.config.js",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "webpack --config webpack.main.config.js --mode production",
    "build:renderer": "webpack --config webpack.renderer.config.js --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test && npm run build && npm run test:e2e",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "package": "electron-builder"
  },
  "dependencies": {
    "@lancedb/lancedb": "^0.4.0",
    "@xenova/transformers": "^2.10.0",
    "chokidar": "^3.5.3",
    "dotenv": "^17.2.3",
    "electron": "^28.0.0",
    "lucide-react": "^0.263.1",
    "ollama": "^0.5.0",
    "pdf-parse": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-window": "^1.8.10",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.1.0",
    "@types/chokidar": "^1.7.5",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^30.2.0",
    "buffer": "^6.0.3",
    "concurrently": "^8.2.0",
    "css-loader": "^7.1.2",
    "electron-builder": "^24.9.0",
    "eslint": "^8.55.0",
    "events": "^3.3.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.7.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "playwright": "^1.57.0",
    "postcss": "^8.4.0",
    "postcss-loader": "^8.2.0",
    "process": "^0.11.10",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0"
  }
}
